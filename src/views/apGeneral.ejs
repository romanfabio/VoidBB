<h1 class="px-0 mt-2">General Settings</h1>

<hr>

<h3 class="mt-3">Change Board Name</h3>

<form method="POST" action="/ap/general">
    <div class="mb-3">
        <label for="boardName" class="form-label">Board Name</label>
        <input type="text" class="form-control" name="boardName" value="<%= boardName %>">
    </div>
    <div class="p-1 text-end">
        <button class="px-3 btn btn-primary">Change</button>
    </div>
</form>